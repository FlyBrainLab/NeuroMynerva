{
  "name": "@flybrainlab/neuany-widget",
  "version": "0.1.0",
  "description": "Template Widget for FlyBrainLab",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "license": "BSD-3-Clause",
  "author": "Tingkai Liu",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib tsconfig.tsbuildinfo",
    "clean:node_modules": "rimraf node_modules lib tsconfig.tsbuildinfo",
    "eslint": "eslint . --ext .ts,.tsx --fix",
    "eslint:check": "eslint . --ext .ts,.tsx",
    "prepare": "jlpm run clean && jlpm run build",
    "watch": "nodemon -e ts,js,tsx --watch src --watch ../fbl-template-widget/lib --exec \"npm run build\"",
    "dev": "concurrently \"npm:watch\""
  },
  "dependencies": {
    "@flybrainlab/fbl-template-widget": "file:../fbl-template-widget",
    "@jupyterlab/application": "^2.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "concurrently": "^5.2.0",
    "css-loader": "^3.5.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "http-server": "^0.12.3",
    "nodemon": "^2.0.4",
    "prettier": "1.16.4",
    "raw-loader": "^4.0.1",
    "rimraf": "^3.0.2",
    "style-loader": "^1.2.1",
    "svg-url-loader": "^5.0.0",
    "typescript": "~3.7.0"
  },
  "sideEffects": [
    "style/*.css"
  ],
  "jupyterlab": {
    "extension": true
  }
}
