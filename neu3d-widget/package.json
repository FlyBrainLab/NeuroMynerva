{
  "name": "@flybrainlab/neu3d",
  "version": "0.1.0",
  "description": "Neu3D Widget",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "license": "BSD-3-Clause",
  "author": "Tingkai Liu",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/my_name/myextension.git"
  },
  "scripts": {
    "build": "tsc && webpack",
    "build:neu3d": "cd ../neu3d && npm run build",
    "install:neu3d": "cd ../neu3d && npm install",
    "watch:neu3d": "cd ../neu3d && npm run watch",
    "clean": "rimraf lib tsconfig.tsbuildinfo",
    "eslint": "eslint . --ext .ts,.tsx --fix",
    "eslint:check": "eslint . --ext .ts,.tsx",
    "prepare": "jlpm run clean && jlpm run build",
    "watch": "nodemon -e ts,js --watch src --watch ../neu3d/lib --exec \"npm run build\"",
    "dev": "concurrently \"http-server -p 7998\" \"npm:watch\" \"npm:watch:neu3d\""
  },
  "dependencies": {
    "@flybrainlab/fbl-template": "file:../fbl-template-widget",
    "@jupyterlab/application": "^2.0.0",
    "neu3d": "file:../neu3d"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "concurrently": "^5.2.0",
    "css-loader": "^3.5.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "http-server": "^0.12.3",
    "nodemon": "^2.0.4",
    "prettier": "1.16.4",
    "raw-loader": "^4.0.1",
    "rimraf": "^2.6.1",
    "style-loader": "^1.2.1",
    "svg-url-loader": "^5.0.0",
    "typescript": "~3.7.0"
  },
  "sideEffects": [
    "style/*.css"
  ],
  "jupyterlab": {
    "extension": true
  }
}
