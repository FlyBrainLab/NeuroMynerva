{
  "name": "@flybrainlab/fbl-extension",
  "version": "0.1.0",
  "description": "FBL Extension.",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "license": "BSD-3-Clause",
  "author": "FlyBrainLab Dev Team",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}",
    "style/widgets/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib tsconfig.tsbuildinfo",
    "clean:node_modules": "rimraf node_modules lib tsconfig.tsbuildinfo",
    "eslint": "eslint . --ext .ts,.tsx --fix",
    "eslint:check": "eslint . --ext .ts,.tsx",
    "watch": "tsc -w",
    "dev": "tsc -w"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.14.0",
    "@jupyterlab/application": "^2.0.0",
    "@jupyterlab/launcher": "^2.1.1",
    "@jupyterlab/ui-components": "^2.1.1",
    "@types/jquery": "^3.3.38",
    "@types/recharts": "^1.8.14",
    "jupyterlab_toastify": "^4.1.0",
    "neu3d": "git+https://github.com/fruitflybrain/neu3d.git#fbl",
    "recharts": "^1.8.5",
    "tabulator-tables": "^4.7.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "css-loader": "^3.5.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "prettier": "1.16.4",
    "raw-loader": "^4.0.1",
    "rimraf": "^3.0.2",
    "style-loader": "^1.2.1",
    "svg-url-loader": "^5.0.0",
    "typescript": "~3.7.0"
  },
  "sideEffects": [
    "style/*.css"
  ],
  "jupyterlab": {
    "extension": true
  }
}
